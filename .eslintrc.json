{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
      "max-len": 0,
      "no-console": 0,
      "consistent-return": "off",
      "arrow-parens": "off",
      "object-curly-newline": "off",
      "class-methods-use-this": "off",
      "max-classes-per-file": "off"
    }
}